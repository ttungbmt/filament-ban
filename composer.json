{
    "name": "ttungbmt/filament-ban",
    "description": "This is my package filament-ban",
    "keywords": [
        "ttungbmt",
        "laravel",
        "filament-ban"
    ],
    "homepage": "https://github.com/ttungbmt/filament-ban",
    "license": "MIT",
    "authors": [
        {
            "name": "Truong Thanh Tung",
            "email": "ttungbmt@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/laravel-package-tools": "^1.16.1",
        "illuminate/contracts": "^10.0",
        "cybercog/laravel-ban": "^4.8",
        "filament/filament": "^3.1.20",
        "filament/forms": "^3.1.20",
        "filament/tables": "^3.1.20"
    },
    "require-dev": {
        "laravel/pint": "^1.13.7",
        "nunomaduro/collision": "^7.10.0",
        "nunomaduro/larastan": "^2.7.0",
        "orchestra/testbench": "^8.17.0",
        "pestphp/pest": "^2.28.0",
        "pestphp/pest-plugin-arch": "^2.5.0",
        "pestphp/pest-plugin-laravel": "^2.2",
        "pestphp/pest-plugin-livewire": "^2.1",
        "phpstan/extension-installer": "^1.3.1",
        "phpstan/phpstan-deprecation-rules": "^1.1.4",
        "phpstan/phpstan-phpunit": "^1.3.15",
        "spatie/laravel-ray": "^1.33"
    },
    "autoload": {
        "psr-4": {
            "FilamentPro\\FilamentBan\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FilamentPro\\FilamentBan\\Tests\\": "tests"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "FilamentPro\\FilamentBan\\FilamentBanServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
